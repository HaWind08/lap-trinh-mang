#include<iostream>
using namespace std;

int main()
{
	int n;
	cout << "Nhap so phan tu cua mang: "; cin >> n;
	
	double a[n];
	cout << "Nhap mang: ";
	for(int i = 0; i < n;i++)
	{
		cin >> a[i];
	}
	
	double max = a[0];
	double min = a[0];
	for(int i = 0; i < n;i++)
	{
		if(a[i] > max)
		{
			max = a[i];
		}
		if(a[i] < min)
		{
			min = a[i];
		}
	}
	cout << "Phan tu lon nhat la: " << max << endl;
	cout << "Phan tu nho nhat la: " << min << endl;
	return 0;
}
